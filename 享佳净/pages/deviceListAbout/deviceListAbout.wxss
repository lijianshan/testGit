page {
  height: 100%;
  width: 100%;
}
.header{
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-content: center;
}
.headertitle{
  margin: 30rpx;
}
.cell {
  width: 100%;
  height: 100rpx;
  color: white;
  font-size: 16px;
  border-bottom: 2rpx solid #f5f5f5;
  background-color: #152642;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.cell image{
  width: 30%;
  height: 60%;
}
.cell text {
  width: 70%;
}

.handing{
  padding: 30rpx;
}
  width: 100%;
}

.cellView {
  width: 90%;
  height: 100rpx;
  margin-top: 30rpx;

  text-align: left;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.border {
  padding-left: 20rpx; 
  padding-right: 20rpx;
  border: 1rpx solid #d5d5d5;
  border-radius: 5px;
  background-color: white;
}

.CAPTCHAText {
  width: 45%;
  height: 100%;
}

.CAPTCHABtn {
  width: 45%;
  height: 100%;
  color: white;
  background-color: #068de7;
}

.registerBtn {
  color: white;
  justify-content: center;
  background-color: #068de7;
}

page {
  height: 100%;
  width: 100%;
}
.wxuserinfoView{
  width: 100%;
  height: 200rpx;
  margin: 20rpx 0 60rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.wximage{
  width: 150rpx;
  height: 150rpx;
  margin: 0 30rpx 0 30rpx;
}
.wxuserinfoView button{
  font-size: 16px;
  color: #545454;
}

.cell {
  width: 100%;
  height: 120rpx;
  border-bottom: 2rpx solid #f5f5f5;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.title {
  margin-left: 30rpx;
}

.iconRight {
  margin-right: 30rpx;
}

.quiteBtn {
  width: 90%;
  background-color: white;
  margin-top: 60rpx;
}

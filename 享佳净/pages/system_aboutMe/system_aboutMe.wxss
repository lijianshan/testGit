page {
  height: 100%;
  width: 100%;
  background-color: #f5f5f5;
}

.iconView {
  width: 100%;
  height: 40%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.iconimg {
  width: 300rpx;
  height: 300rpx;
  border: 0 solid #f00;
  border-radius: 20rpx;
}

.cell {
  width: 100%;
  height: 120rpx;
  font-size: 16px;
  border-bottom: 2rpx solid #f5f5f5;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.iconLeft {
  width: 10%;
  height: 50%;
  padding-left: 10rpx;
}

.title {
  width: 20%;
  padding-left: 10rpx;
}

.value {
  width: 60%;
  padding-left: 10rpx;
  padding-right: 10rpx;
}

.iconRight {
  padding-left: 10rpx;
  padding-right: 10rpx;
  width: 5%;
  height: 100%;
}

page {
  width: 100%;
  height: 100%;
}

.container{
  margin: 0 30rpx 0 30rpx;
  display: flex;
  flex-direction: column;
}
.dividingLine {
  margin-top: 50rpx;
  width: 100%;
  height: 5rpx;
  background-color: lightslategrey;
}

.title {
  margin-top: 20rpx;
  text-align: left;
  font-size: 18px;
  font-weight: 600;
}

.sncell {
  width: 100%;
  font-size: 14px;
  border-bottom: 2rpx solid #f5f5f5;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.modifyBtn {
  color:  white; 
   background-color: #068de7;
  margin-right: 10rpx;
}

.addBtn {
  margin-top: 10rpx;
  margin-left: 0rpx;
  color: white;
  background-color: #068de7;
}

page {
  width: 100%;
  height: 100%;
}

.cutView {
  height: 50rpx;
}

.cellView {
  width: 90%;
  height: 100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.cellView button {
  font-size: 14px;
}

.input {
  margin-bottom: 30rpx;
  border: 1rpx solid #d5d5d5;
  border-radius: 5px;
  color: #545454;
  background-color: white;
  text-align: center;
}

.setBtn {
  justify-content: center;
  color: white;
  background-color: #068de7;
}

.warning{
  margin-top: 50rpx;
  color: red;
}

.wifiListMask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
}

.wifiList {
  width: 80%;
  height: 80%;
  border-radius: 30rpx;
  position: fixed;
  top: 10%;
  left: 10%;
  z-index: 9001;
  background-color: black;
  color: #dfc8a0;
  display: flex;
  flex-direction: column;
}

.title{
  color: white;
  justify-content: center;
}
.cell{
  width: 100%;
  height: 15%;
  display: flex;
  align-items: center;
}

.wifiList scroll-view{
  width: 100%;
  height: 70%;
  border-top: 2rpx solid #f5f5f5;
  border-bottom: 2rpx solid #f5f5f5;
}

.cell image{
  width: 20%;
  height: 60%;
}
.cancelBtn{
  width: 400rpx;
  margin-top: 30rpx;
  color: white;
  background-color: #282d30;
}

.versionsView {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  position: fixed;
  top: 0;
  left: 0;
  overflow: hidden;
  z-index: 9002;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.versionsView text{
  margin-top: 40%;
  width: 80%;
}

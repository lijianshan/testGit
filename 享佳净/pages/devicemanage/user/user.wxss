page {
  width: 100%;
  height: 100%;
}

.title {
  width: 100%;
  height: 120rpx;
  margin-left: 30rpx;
  color: #1296db;
  font-size: 20px;
  font-weight: 800;
  display: flex;
  align-items: center;
}

.cell {
  width: 100%;
  height: 120rpx;
  border-bottom: 2rpx solid #f5f5f5;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.user {
  margin-left: 50rpx;
}

.iconRight {
  margin-right: 50rpx;
}
